    "correctAnswer": "true",
    "category": "仮免-2",
    "hash": "269ac3f6dfb5b0aa11111c4696c59b197a695cbc66c686632cb8beab0be6d4fa",
    "license_tags": [
      "all"
    ],
    "stage_tag": "both",
    "topic_tags": [
      "intersection",
      "parking_stopping"
