{
  "name": "nextjs-react-typescript-starter",
  "private": true,
  "version": "1.0.8",
  "scripts": {
    "dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "check:build": "./scripts/check-build.sh",
    "check:types": "tsc --noEmit",
    "fix:build": "tsx scripts/auto-fix-build-errors.ts",
    "normalize-tags": "tsx scripts/normalize-question-tags.ts",
    "restore-tags": "tsx scripts/restore-tags-from-latest-package.ts",
    "duplicate-latest": "tsx scripts/duplicate-from-latest-package.ts",
    "cf:build": "npm install --include=optional && npx @opennextjs/cloudflare build",
    "cf:preview": "npm install --include=optional && npx @opennextjs/cloudflare build && npx wrangler dev",
    "cf:deploy": "npm install --include=optional && npx @opennextjs/cloudflare build && npx wrangler deploy"
  },
  "dependencies": {
    "@auth/core": "^0.41.1",
    "@auth/kysely-adapter": "^1.0.0",
    "jose": "^6.1.0",
    "kysely": "^0.28.8",
    "lucide-react": "^0.344.0",
    "next": "15.5.7",
    "next-auth": "^5.0.0-beta.30",
    "pg": "^8.11.0",
    "qrcode": "^1.5.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@opennextjs/cloudflare": "^1.12.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.11.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitest/ui": "^4.0.6",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "eslint": "^9.0.0",
    "eslint-config-next": "^15.0.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.4.35",
    "rimraf": "^6.1.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.0",
    "typescript": "^5.5.3",
    "vitest": "^4.0.6",
    "wrangler": "^4.47.0"
  },
  "optionalDependencies": {
    "@ast-grep/napi-linux-x64-gnu": "^0.35.0"
  },
  "overrides": {
    "rimraf": "^6.1.0",
    "glob": "^10.3.10",
    "@auth/kysely-adapter": {
      "kysely": "^0.27.5"
    }
  },
  "npmAuditConfig": {
    "audit-level": "moderate"
  }
}
